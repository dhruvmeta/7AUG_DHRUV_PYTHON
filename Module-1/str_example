fnam = input("enter your firstname")
lnam = input("enter your lastname")

if fnam.isupper() and lnam.isupper():
    print("good.. now enter your email address!")
    email=input("Enter an email")
    if email.islower():
        print("look's good... your data has been submitted")
        pad = input("enter your password:")
        cpad =input("enter the password:")
        if pad==cpad:
            print("Proceed further...")
            no=input("enter the number:")
            if no.isdigit() and len(no)==10:
                print("your procced done!")
            else:
                print("error! plz input proper no")
        else:
            print("error! reenter your password")    
    
    else:
        print("error! plz unput proper email addres") 
else:
    print("error! plz input firtsname and lastname in capital only ")


